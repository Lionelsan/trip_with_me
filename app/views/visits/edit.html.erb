<%= simple_form_for @visit do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :price %>
  <%= f.input :place %>
  <%= f.input :tag %>
  <%= f.input :photo, as: :file %>
  <%= f.button :submit, class: 'btn btn-primary' %>
<% end %>

<%= link_to 'Back', visits_path, class: 'btn btn-primary' %>